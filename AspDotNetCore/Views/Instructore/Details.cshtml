@model Instructore
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<table class="table table-bordered table-hover">

    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Image</th>
        <th>Salary</th>
        <th>Address</th>
    </tr>
    <tr>
        <th>@Model.id</th>
        <th>@Model.Name</th>
        <th>@Model.Image</th>
        <th>@Model.Salary</th>
        <th>@Model.Address</th>
    </tr>
    
</table>